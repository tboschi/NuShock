#set term qt size 1500, 525
set term epslatex color size 10in, 3.5in linewidth 2
set output "sensmulti_TW2.tex"

#ytics margin size
MT = 0.04
#spacing between plots
MP = (1-MT)/3.0

set colorsequence classic
set tmargin 0
set rmargin 0
set bmargin 2
#set lmargin 0
set xtics 0.01, 10, 10 nomirror
set xtics add (0.05, 0.5)
set ytics 1e-12, 100, 1 nomirror
set xlabel "Mass (GeV)"
set log xy
set xrange [0.01:2]
set yrange [1e-7:1]
set key samplen 1 spacing 1.2 bottom left reverse Left
#set multiplot layout 1, 3 spacing screen 0
set multiplot
#plot 1
set key at 0.015, 5e-5 top left
set lmargin at screen MT+0.5*MP
set size MT+1.5*MP, 1
set origin 0, 0
set format y "\\np\{e%T\}"
set ylabel "$ |U_{\\tau 4}|^2 $" 
plot "m/nEE_T_m_U.dat"   u 1:2 w l lw 2 lc rgb "black" t "\$\\nu e e \$ Majorana",\
     "m/nEE_W_T_m_U.dat" u 1:2 w l lw 2 lc rgb "black" dt "-" not,\
     "d/nEE_T_d_U.dat"   u 1:2 w l lw 2 lc rgb "blue" t "\$\\nu e e \$ Dirac",\
     "d/nEE_W_T_d_U.dat" u 1:2 w l lw 2 lc rgb "blue" dt "-" not, \
     "m/nMM_T_m_U.dat"   u 1:2 w l lw 2 lc rgb "red" t "\$\\nu \\mu \\mu \$ Majorana",\
     "m/nMM_W_T_m_U.dat" u 1:2 w l lw 2 lc rgb "red" dt "-" not,\
     "d/nMM_T_d_U.dat"   u 1:2 w l lw 2 lc rgb "web-green" t "\$\\nu \\mu \\mu \$ Dirac",\
     "d/nMM_W_T_d_U.dat" u 1:2 w l lw 2 lc rgb "web-green" dt "-" not
#plot 2
#set lmargin 0
#set size MP, 1
#set origin MT+MP, 0
#set format y ""
#unset ylabel
#plot "m/EPI_T_m_U.dat" u 1:2 w l t "\$e \\pi \$ Majorana" lw 2 lc 1, "m/EPI_W_T_m_U.dat" u 1:2 w l not lw 2 lc 1 dt "-", "d/EPI_T_d_U.dat" u 1:2 w l t "\$e \\pi \$ Dirac" lw 2 lc 2, "d/EPI_W_T_d_U.dat" u 1:2 w l not lw 2 lc 2 dt "-", \
#"m/nEM_T_m_U.dat" u 1:2 w l t "\$\\nu e \\mu \$ Majorana" lw 2 lc 6, "m/nEM_W_T_m_U.dat" u 1:2 w l not lw 2 lc 6 dt "-", "d/nEM_T_d_U.dat" u 1:2 w l t "\$\\nu e \\mu \$ Dirac" lw 2 lc 7, "d/nEM_W_T_d_U.dat" u 1:2 w l not lw 2 lc 7 dt "-"
#plot 3
#set xrange [0.05:2]
set key at 0.015, 5e-5 top left
set lmargin 0
set size MP, 1
set origin MT+1.5*MP, 0
set format y ""
unset ylabel
set xtics add (2)
plot "m/nPI0_T_m_U.dat"   u 1:2 w l lw 2 lc rgb "black" t "\$\\nu \\pi^0 \$ Majorana",\
     "m/nPI0_W_T_m_U.dat" u 1:2 w l lw 2 lc rgb "black" dt "-" not,\
     "d/nPI0_T_d_U.dat"   u 1:2 w l lw 2 lc rgb "blue" t "\$\\nu \\pi^0 \$ Dirac",\
     "d/nPI0_W_T_d_U.dat" u 1:2 w l lw 2 lc rgb "blue" dt "-" not
unset multiplot
unset output
